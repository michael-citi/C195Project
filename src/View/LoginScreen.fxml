<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="232.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginScreenController">
   <children>
      <TextField fx:id="uNameTextField" layoutX="184.0" layoutY="76.0" prefHeight="25.0" prefWidth="224.0" />
      <PasswordField fx:id="pWordTextField" layoutX="184.0" layoutY="116.0" prefHeight="25.0" prefWidth="224.0" />
      <Button id="loginBtn" fx:id="loginBtn" defaultButton="true" layoutX="321.0" layoutY="165.0" mnemonicParsing="false" onAction="#checkCredentials" prefHeight="36.0" prefWidth="77.0" text="OK" />
      <Button id="cancelBtn" fx:id="cancelBtn" cancelButton="true" layoutX="117.0" layoutY="165.0" mnemonicParsing="false" onAction="#cancel" prefHeight="36.0" prefWidth="77.0" text="Cancel" />
      <Label id="uNameLabel" fx:id="uNameLabel" alignment="CENTER" layoutX="41.0" layoutY="71.0" prefHeight="36.0" prefWidth="110.0" text="User Name">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label id="pWordLabel" fx:id="pWordLabel" alignment="CENTER" layoutX="37.0" layoutY="111.0" prefHeight="36.0" prefWidth="110.0" text="Password">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label id="welcomeLabel" fx:id="welcomeLabel" layoutX="169.0" layoutY="19.0" text="Welcome! Please Log In" textAlignment="CENTER" textFill="#050a97">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
